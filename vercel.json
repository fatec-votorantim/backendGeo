{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/doc/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api"
        }
    ],
    "functions": {
        "api/index.js": {
            "includeFiles": "api/swagger/swagger_output.json"
        }
    }
}