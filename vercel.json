{
  "version": 2,
  "builds": [
    {
      "src": "index.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/doc/(.*)",
      "dest": "/index.js" 
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run build" 
}